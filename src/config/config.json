{
  "site": {
    "title": "PickNWear - Official Online Store",
    "base_url": "https://picknwear-public-production.up.railway.app/",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.ico",
    "logo": "/images/logo.png",
